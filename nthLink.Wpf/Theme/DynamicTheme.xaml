    <Thickness x:Key="BubbleInfoMargin">0,10,0,0</Thickness>
    <sys:Double x:Key="BubbleInfoWidth">180</sys:Double>
    <sys:Double x:Key="BubbleInfoHeight">40</sys:Double>
    <CornerRadius x:Key="BubbleInfoCornerRadius">20</CornerRadius>

    <sys:Double x:Key="BubbleInfoArrowWidth">12</sys:Double>
